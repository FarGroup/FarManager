ROOTDIR = ../../..

ANSI = 1

NAME = ExcDump
LINK_OBJS = $(INTDIR)\ExcDump.obj

COMMON = ..\..\..\plugins\common

XXLIB=$(INTDIR)\xxLib.lib
OBJLIB=$(INTDIR)\LibObj
LIBSRC=xxlib\std

