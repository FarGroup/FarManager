<?xml version="1.0" encoding="UTF-8" ?>
<farconfig>
  <macros>
    <keymacros>
      <macro area="Viewer" key="Space" flags="DisableOutput" description="Use Space for listing in the viewer (like in Outlook Express or The Bat!)">
<text>
  if Object.Eof then
    Keys('CtrlF10 Shift')
    if not APanel.Eof then Keys('Add Home') end
  else
    Keys('PgDn')
  end
</text>
      </macro>
    </keymacros>
  </macros>
</farconfig>
