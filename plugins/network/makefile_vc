NAME = Network
LINK_OBJS = $(OBJDIR)\Network.obj \
$(OBJDIR)\NetCfg.obj \
$(OBJDIR)\NetClass.obj \
$(OBJDIR)\NetCommon.obj \
$(OBJDIR)\NetFavorites.obj \
$(OBJDIR)\NetMix.obj \
