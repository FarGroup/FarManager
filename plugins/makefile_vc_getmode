!ifdef __MAKE__
_MKDO_=/D
!endif

!ifdef WIDE
_MDEFS=$(_MDEFS) $(_MKDO_)WIDE=1
!endif

!ifdef DEBUG
_MDEFS=$(_MDEFS) $(_MKDO_)DEBUG=1
!endif

!ifdef NO_ULINK
_MDEFS=$(_MDEFS) $(_MKDO_)NO_ULINK=1
!elseif defined(ULINK)
_MDEFS=$(_MDEFS) $(_MKDO_)ULINK=1
!endif

!if defined(AMD64) || "$(CPU)" == "AMD64"
_MDEFS=$(_MDEFS) $(_MKDO_)AMD64=1
!endif
